{
    "name": "andrey-helldar/strong-password",
    "description": "This package provides a validator for ensuring strong passwords in Laravel applications",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "password", "passwords", "strength", "strong", "security", "validation"],
    "support": {
        "issues": "https://github.com/andrey-helldar/strong-password/issues",
        "source": "https://github.com/andrey-helldar/strong-password"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0",
        "illuminate/validation": "^5.5|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\StrongPassword\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "suggest": {
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\StrongPassword\\ServiceProvider"
            ]
        }
    }
}
